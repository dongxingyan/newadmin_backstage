login {
  @font-face {
    font-family: 'iconfont';  /* project id 247379 */
    src: url('https://at.alicdn.com/t/font_c70m7wid6cuzbyb9.eot');
    src: url('https://at.alicdn.com/t/font_c70m7wid6cuzbyb9.eot?#iefix') format('embedded-opentype'),
            url('https://at.alicdn.com/t/font_c70m7wid6cuzbyb9.woff') format('woff'),
            url('https://at.alicdn.com/t/font_c70m7wid6cuzbyb9.ttf') format('truetype'),
            url('https://at.alicdn.com/t/font_c70m7wid6cuzbyb9.svg#iconfont') format('svg');
  }
  .iconfont {
    font-family: "iconfont";
    font-size: 18px;
    margin-right: 10px;
    font-style: normal;
    color: #AEB9C2;
  }
  a {
    text-decoration: none;
    color: rgb(43, 50, 69);
  }
  .pf55 {
    padding-left: 55px;
  }
  .pr {
    position: relative
  }
    .opacity1{
      opacity:1;
    }
      .opacity0{
        opacity 0;
      }
  .content {
    width: 100%
    height: 100vh;
    background url("../../static/images2/bodybg.png") left top no-repeat;
    background-size cover;
    .logo{
      position fixed
      left: 40px;
      top: 26px;
      width: 220px;
      height: 60px;
      background: url("../../static/images2/logo.png") no-repeat;
      background-size: 100%;
    }
    .container {
      height: 100%
      display: -webkit-box; /* 老版本语法: Safari, iOS, Android browser, older WebKit browsers. */
      display: -moz-box; /* 老版本语法: Firefox (buggy) */
      display: -ms-flexbox; /* 混合版本语法: IE 10 */
      display: -webkit-flex; /* 新版本语法: Chrome 21+ */
      display: flex;
      -webkit-box-pack: center;
      -moz-justify-content: center;
      -webkit-justify-content: center;
      justify-content: center;
      -webkit-box-align: center;
      -moz-align-items: center;
      -webkit-align-items: center;
      align-items: center;

      .left {
        width: 447px
        height: 200px
        background url("../../static/images1/title.png") left top no-repeat
        background-size 100%
      }
      .right {
        width: 380px
        height: 400px
        //margin-right: 100px;
        background url("../../static/images1/loginbcg.png") left top no-repeat
        .loginTip{
          display: block
          width: 340px
          height: 25px
          line-height: 25px
          margin-left:20px;
          text-align center
          margin-top:130px;
          color:red;
          background:pink
        }
      //background:pink
        .iconfont {
          position absolute
          top: 0;
          left: 0
        }
        .inputBox {
          margin: 30px 20px 0;
        }
        .inputCommon {
          width: 340px
          height: 40px
          border-radius 5px;
          border: none
        }
        .common::before {
          display: block
          position: absolute
          width: 29px
          height: 26px

        }
        .userName {
          margin-top:5px;
        }
        .userName::before {
          top: 8px;
          left: 17px;
          content: url("../../static/images1/user.png")
        }
        .password::before {
          bottom: 8px;
          left: 17px;
          content: url("../../static/images1/password.png")
        }
        .checkCode {
          display: -webkit-box; /* 老版本语法: Safari, iOS, Android browser, older WebKit browsers. */
          display: -moz-box; /* 老版本语法: Firefox (buggy) */
          display: -ms-flexbox; /* 混合版本语法: IE 10 */
          display: -webkit-flex; /* 新版本语法: Chrome 21+ */
          display: flex;
          -webkit-box-pack: center;
          -moz-justify-content: center;
          -webkit-justify-content: center;
          justify-content: center;
          .inputCode {
            width: 230px
          }
          .code {
            width: 95px
            height: 40px
            line-height: 40px;
            text-align center
            border-radius 5px;
            background #fff;

          }
        }
        .checkCode::before{
          top: 7px;
          left: 17px;
          content: url("../../static/images1/code.png")
        }
        .loginBtn {
          background: #5EC724;
          color: #fff;
          cursor pointer
        }

      }
    }
  }
}
