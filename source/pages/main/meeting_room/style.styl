main-meeting-room {
  width: 100%
  height: 100%
  .ml20 {
    margin-left: 20px;
  }
    a:hover{
      text-decoration none
    }
  .main {
  //width: 100%
    height: 100vh;
    display: flex
  //background: pink;
    flex-direction column
    font-family: "Helvetica Neue", "Luxi Sans", "DejaVu Sans", Tahoma, "Hiragino Sans GB", STHeiti, "Microsoft YaHei";
    font-size: 14px;
    .current {
      background: #0099CC
      border: none
    }
    .normal{
      background #fff;
    }
    .paging {
      margin-top: 30px;
      margin-bottom: 20px;
      height: 26px
      line-height: 26px
      display flex
      justify-content: center
      i {
        font-style normal
        color: red;
      }
      .page-common {
        font-size: 12px
        display: block
        width: 26px
        height: 26px
        line-height: 26px
        text-align center
        border: 1px solid #aaaaaa
        margin-right: 5px;
        color: #393f51
        cursor pointer
      //border-radius 5px;
      }
      .before {
        margin-left: 10px;
      }
      .after {
        margin-right: 20px;
      }
      .bg {
        background-color: rgba(128, 128, 128, .3)
        border: none;
      }
      ul {
        padding-left: 0;
      }
      li {
        float: left;
      }
      .page-common:hover {
        background: #ccc;
      }
      .pageNum {
        list-style none
      }
      .current {
        background: #0099CC
        border: none
      }
      .skip {
        width: 30px
        height: 26px
        line-height: 26px
      //text-align center
        border: 1px solid #aaaaaa
        margin-right: 5px;
        color: #393f51
      }
      .skipButton {
        width: 35px
        height: 26px
        text-align center
        cursor pointer
        background: #0099CC
      }

    }
  }
}