animtime = .3s
@keyframes loading {
  0% {
    border-bottom-color #fcf8e3;
  }
  50% {
    border-bottom-color orange;
  }
  100% {
    border-bottom-color #fcf8e3;
  }
}

@keyframes creating {
  0% {
    border-bottom-color #fcf8e3;
  }
  50% {
    border-bottom-color lightgreen;
  }
  100% {
    border-bottom-color #fcf8e3;
  }
}

@keyframes removing {
  0% {
    border-bottom-color #fcf8e3;
  }
  50% {
    border-bottom-color indianred;
  }
  100% {
    border-bottom-color #fcf8e3;
  }
}
.loading {
  animation: loading 1s infinite;
}

.creating {
  animation: creating 1s infinite;
}

.removing {
  animation: removing 1s infinite;
}

