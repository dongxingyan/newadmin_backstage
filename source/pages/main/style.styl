// 重写消息提示框样式
.info-modal {
    min-width: 250px;
    text-align: center;
    position: absolute;
    right: 32px;
    z-index: 9999;
    height: 50px;
    font-size: 16px;
    top: 14%;

  .glyphicon{
    left: -20px;
    top: 3px;
  }
}

.notification {
  position: fixed;
  padding: 20px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-border-radius: 2px;
  -moz-border-radius: 2px;
  border-radius: 2px;
  width: 330px;
  height: 100px;
  right: 16px;
  top: 16px;
  z-index: 2008;
  background: #fff;
  box-shadow: 0 2px 4px rgba(0,0,0,.12), 0 0 6px rgba(0,0,0,.04);
  transition: opacity .3s,transform .3s,right .3s,top .4s;
  overflow: hidden;
  .text {
    margin-left: 55px;
    .title {
      font-weight: 400;
      font-size: 16px;
      color: #1f2d3d;
      margin: 0;
    }
    .content {
      font-size: 14px;
      line-height: 21px;
      margin: 10px 0 0;
      color: #8391a5;
      text-align: justify;
    }
  }
  .icon {
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
    width: 40px;
    height: 40px;
    float: left;
    position: relative;
    top: 3px;
    span {
      color: #ffffff;
      line-height: 40px;
      margin-left: 31%;
      margin-right: 31%;
    }
  }
  .success {
    background: #13ce66;
  }
  .error {
    background: #ff4949;
  }
}
@keyframes loading {
  0% {
    border-bottom-color #fcf8e3;
  }
  50% {
    border-bottom-color orange;
  }
  100% {
    border-bottom-color #fcf8e3;
  }
}

@keyframes creating {
  0% {
    border-bottom-color #fcf8e3;
  }
  50% {
    border-bottom-color lightgreen;
  }
  100% {
    border-bottom-color #fcf8e3;
  }
}

@keyframes removing {
  0% {
    border-bottom-color #fcf8e3;
  }
  50% {
    border-bottom-color indianred;
  }
  100% {
    border-bottom-color #fcf8e3;
  }
}

.loading {
  animation: loading 1s infinite;
}

.creating {
  animation: creating 1s infinite;
}

.removing {
  animation: removing 1s infinite;
}

