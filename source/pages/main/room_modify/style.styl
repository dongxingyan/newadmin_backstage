main-room-modify {
  width: 100%
  height: 100%
  .ml20 {
    margin-left: 20px;
  }
  .main {
  //width: 100%
    position: relative;
    height: 100%
    display: flex
  //background: pink;
    flex-direction column
    font-family: "Helvetica Neue", "Luxi Sans", "DejaVu Sans", Tahoma, "Hiragino Sans GB", STHeiti, "Microsoft YaHei";
    font-size: 12px;
    .feedback{
      z-index 2
      position absolute
      width: 100%
      height: 100%
      display: flex
      justify-content center
      align-items center
      background:rgba(0,0,0,.5)
      .alerts{
        width: 300px;
        height: 260px;
        background:#fff;
        border-radius: 5px;
        .alert-head{
          height: 40px;
          background: #31a3dd;
          line-height: 40px;
          padding-left: 35px;
          color: #ffffff;
          font-size: 12px;
          position: relative;
          border-radius: 5px 5px 0px 0px;
          .img-right,.delet-right,.add-right,.page-right{
            position: absolute;
            right: 6px;
            top: 8px;
            height: 25px;
          }
        }
        .img-left{
          position: absolute;
          left: 10px;
          top: 10px;
          height: 21px;
        }
        .alert-content{
          height: 110px;
          line-height: 110px;
          text-align: center;
          font-family: "微软雅黑";
          font-size: 16px;

        }
        .alert-content1{
          height: 165px;
          text-align: center;
          font-family: "微软雅黑";
          font-size: 16px;
          label{
            display: block;
          }
          .tipContent{
            height: 95px
            overflow-y:scroll
          }
        }
        .alert-bottom span{
          display: inline-block;
          text-align: center;
          padding: 5px;
          height: 30px;
          width: 80px;
          cursor: pointer;
          border-radius: 5px;
        }
        .alert-sure,.delet-sure,.add-sure,.page-sure{
          background:#31a3dd;
          color: #fff;
        }
        .alert-bottom{
          text-align: center;
        }
      }

    }

    .title {
      font-size: 14px;
      height: 50px
      margin-left: 20px
      margin-top: 20px;
      line-height: 50px
      background: #fff;
      ul {
        list-style none
        .nav-icon {
          margin-left: 5px;
          margin-right: 5px;
        }
        li {
          float: left;
          a {
            text-decoration none;
            color: #09c;
          }
        }
      }
    }
    .join-participant-permission {
      display: flex;
      width: 200px;
      align-items: center;
      input {
        width: 19px;
        height: 19px;
        margin-right: 5px;
      }
      label {
        margin-top: 6px;
      }
    }
    .create-broadcast {
      display: flex;
      flex-direction: column;
      justify-content center;
      align-items center;
    }
    .box-room {
      height: 25px
      margin: 16px;
      .titleLeft{
        display: inline-block
        width: 100px
        height: 25px
        line-height: 25 px
        text-align right
        margin-right: 20px
        i {
          font-style normal
          color: red;
        }
      }
        .inputBox{
          width: 200px
          height: 30px
          padding-left: 5px;
        }
        .no{
          margin-left:30px;
        }
    }
      .describe{
        margin: 20px;
        display flex
        .titleLeft{
          display: inline-block
          width: 100px
          height: 25px
          line-height: 25 px
          text-align right
          margin-right: 20px;
        }
        .anotherName{
          //vertical-align top
          border: 1px solid #aaa
          width: 200px;
          height: 150px;
          overflow auto;
        }
      }

    .bottom {
      margin-bottom:20px;
      display flex
      justify-content center;
      .common {
        width: 140px
        height: 40px
        border-radius 5px
      }
      #save {
        margin-right: 30px
        background: #169BD5;
        border: none;
        color: #ffffff;
        cursor: pointer;
      }
      #cancel {
        border: 1px solid #aaa
        background: #fff;
        cursor: pointer;
      }
    }
  }
}