main-broadcast-count {
  width: 100%
  height: 100%
  .ml20 {
    margin-left: 20px;
  }
  .main {
    font-size: 14px;
    .uc-no-data-container{
      display: -webkit-box;
      display: -ms-flexbox;
      display: flex;
      -webkit-box-orient: vertical;
      -webkit-box-direction: normal;
      -ms-flex-direction: column;
              flex-direction: column;
      -webkit-box-pack: center;
      -ms-flex-pack: center;
              justify-content: center;
      -webkit-box-align: center;
      -ms-flex-align: center;
              align-items: center;

      .uc-no-data-title {
        height: 50px;
        line-height: 50px;
        font-size: 18px;
        font-weight: bold;
      }
      .uc-no-data-img {
        width:160px;
        height: 200px;
        background: url("../../../static/images/no-data.png") center center no-repeat;
      }
    }
    .broadcast-count-box-right-header {
      background: #ffffff;
      padding-left: 20px;
      display: flex;
      flex-direction: column;
      border-radius: 5px;
      .broadcast-count-text-container {
        display: flex;
        align-items: center;
        padding-left: 16px;
        .broadcast-count-nav-icon {
          background: #31a3dd;
          width: 4px;
          height: 19px;
          -webkit-border-radius: 10px;
          -moz-border-radius: 10px;
          border-radius: 10px;
        }
        .right-title {
          height: 65px;
          line-height: 65px;
          display: flex;
          justify-content: space-between;
          font-size: 18px;
          color: #646464;
          text-indent: 10px;
          font-weight: bold;
        }
      }
      .search-condition-container {
        display: flex;
        padding-bottom: 18px;
        .time-button-container {
          display: flex;
          border: 1px solid #ccc;
          margin-left: 16px;
          height: 37px;
          .selected {
            background: #1d94f5 !important;
            color: #ffffff !important;
          }
          div {
            background: #fff;
            height: 35px;
            line-height: 35px;
            text-align: center;
            border: 0;
            padding-left: 2px;
            padding-right: 2px;
            cursor: pointer;
            width: 70px;
          }
          div:hover {
            background: #1d94f5 !important;
            color: #ffffff !important;
          }
        }
        .search-time-container {
          margin-left: 2%;
          display: flex;
          div {
            width: 36px;
            display: flex;
            justify-content: center;
            align-items: center;
          }
          input {
            height: 37px;
            line-height: 37px;
            border: 1px solid #ccc;
            width: 100px;
            text-align: center;
          }
          i {
            display: inline-block;
            width: 25px;
            background: #a9a9a9;
            height: 1px;
            margin-right: 5px;
            margin-left: 5px;
            margin-top: 15px;
          }
        }
        .meeting-search-select-container select {
          border: 1px solid #ccc;
          height: 35px;
          width: 170px;
          margin-left: 10px;
          padding-left: 10px;
        }
        .search-button {
          font-size: 16px;
          text-align center
          color: #68788c;
          height: 35px;
          line-height: 35px;
          cursor: pointer;
          &:hover {
            color: #49728c;
          }
        }
        .output-table-button {
          font-size:16px;
          color: #31a3dd;
          height: 35px;
          line-height: 35px;
          text-align: center;
          cursor: pointer;
          background: #fff;
          &:hover {
            color: #1b729f;
          }
        }
      }
    }
    .row {
      display: flex;
      background: #ffffff;
      margin-left: 0;
      margin-right: 0;
      padding-left: 20px;
      border-radius: 5px;
      margin-top: 22px;
      .live-concurrent-chart-container {
        margin-left: 15px;
        width: 100%;
        margin-top: 30px;
        padding-top: 20px;
        margin-right: 1%;
        div {
          display: flex;
          justify-content: center;
          align-items: center;
        }
      }
      .live-user-zone-chart-container {
        margin-left: 15px;
        width: 100%;
        margin-top: 30px;
        padding-top: 20px;
        margin-right: 1%;
        div {
          display: flex;
          justify-content: center;
          align-items: center;
        }
      }
      .live-user-device-chart-container {
        margin-left: 15px;
        width: 100%;
        margin-top: 30px;
        margin-right: 1%;
        div {
          display: flex;
          justify-content: center;
          align-items: center;
        }
      }
      .live-browser-version-chart {
        margin-left: 15px;
        width: 100%;
        margin-top: 30px;
        padding-top: 20px;
        margin-right: 1%;
        div {
          display: flex;
          justify-content: center;
          align-items: center;
        }
      }
      .live-flow-chart-container {
        margin-left: 15px;
        width: 100%;
        margin-top: 30px;
        padding-top: 20px;
        margin-right: 1%;
        div {
          display: flex;
          justify-content: center;
          align-items: center;
        }
      }
    }
    th {
      font-size: 16px;
      text-align: center;
    }
    tr {
      line-height: 40px;
    }

    button:hover {
      background: #1b729f;
    }
    .warn-info-tip-modal {
      position: fixed;
      top: 0;
      left: 0;
      bottom: 0;
      right: 0;
      z-index: 9999;
      background: rgba(0,0,0, 0.1);
    }
    .warn-type-container {
      width: 315px;
      height: 72px;
      background: #ffffff;
      top: 40%;
      left: 40%;
      right: 50%;
      bottom: 50%;
      position: absolute;
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 14px;
      border-radius: 3px;
    }
    .warn-type-container img {
      margin-right: 14px;
    }
  }
  .uc-hide{
    display:none !important;
  }
}
