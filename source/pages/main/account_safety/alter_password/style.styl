main-alter-password {
  width: 100%
  height: 100%
  .ml20 {
    margin-left: 20px;
  }
  .main {
      @import '../common.styl';
    font-size: 14px;
    .alerts{
      width: 300px;
      height: 210px;
      background:#fff;
      border-radius: 5px;
      .alert-head{
        height: 40px;
        background: #31a3dd;
        line-height: 40px;
        padding-left: 35px;
        color: #ffffff;
        font-size: 12px;
        position: relative;
        border-radius: 5px 5px 0px 0px;
        .img-right,.delet-right,.add-right,.page-right{
          position: absolute;
          right: 6px;
          top: 8px;
          height: 25px;
        }
      }
      .img-left{
        position: absolute;
        left: 10px;
        top: 10px;
        height: 21px;
      }
      .alert-content{
        height: 110px;
        line-height: 110px;
        text-align: center;
        font-size: 16px;

      }
      .alert-bottom span{
        display: inline-block;
        text-align: center;
        padding: 5px;
        height: 30px;
        width: 80px;
        cursor: pointer;
        border-radius: 5px;
      }
      .alert-sure,.delet-sure,.add-sure,.page-sure{
        background:#31a3dd;
        color: #fff;
      }
      .alert-bottom{
        text-align: center;
      }
    }
  }
}
