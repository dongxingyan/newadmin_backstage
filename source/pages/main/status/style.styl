main-status {
  width: 100%
  height: 100%
  .ml20 {
    margin-left: 20px;
  }
  .main {
  //width: 100%
    height: 100%
    display: flex
  //background: pink;
    flex-direction column
    font-family: "Helvetica Neue", "Luxi Sans", "DejaVu Sans", Tahoma, "Hiragino Sans GB", STHeiti, "Microsoft YaHei";
    font-size: 14px;
    .title {
      font-size: 14px;
      height: 50px
      margin-left: 20px
      margin-top: 20px;
      line-height: 50px
      background: #fff;
      box-shadow 0 2px 2px 0 #ccc
      span {
        border-left: 2px solid #88B7E0
        padding-left: 10px;
      }
    }
    .box {
      flex: 1;
      display: flex
      flex-direction column
      margin: 0 30px;
      border-radius: 5px;
      .right-top {
        display: flex;
        justify-content: space-between;
        .organize{
          width: 50%;
          height: 280px;
          border-radius: 8px;
          background-color: #fff;
          .count {
            font-size: 40px;
            padding-left: 15px;
            color: #fb8342;
          }
          i {
            font-style: normal;
            font-size: 14px;
            color: #42a8fb;
          }
          //table {
          //  width: 100%;
