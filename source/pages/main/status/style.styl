            .box-title td {
              padding: 15px;
              background: url("../../../static/images2/react.png") left center no-repeat;
              font-size: 14px;
              line-height: 25px;
              color: #000;
            }
          }
        }
        .content-right {
          width: 50%;
          margin-left: 20px;
          border-radius: 8px;
          background: #fff;
          .count {
            font-size: 40px;
            padding-left: 15px;
            color: #fb8342;
          }
          i {
            font-style: normal;
            font-size: 14px;
            color: #42a8fb;
          }
          .count-meetingroom table {
            width: 100%;
            height: 140px;
            padding: 20px;
            font-size: 12px;
            color: #68788c;
            margin-left:30px;
            td {
              width: 15%;
              line-height: 25px;
            }
            .box-title td {
              padding: 15px;
              background: url("../../../static/images2/react.png") left center no-repeat;
              font-size: 14px;
              color: #000;
            }
          }

