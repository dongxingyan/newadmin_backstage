            }
            .titleContent {
              width: 190px
            }
            .inputBox {
              width: 163px
            }
          }
        }

        .box-describe {
          .box-left {
            line-height: 30px
          }
          .box-right {
            textarea {
              width: 240px;
              height: 70px;
              background: rgba(255, 255, 255, 1);
              border-radius: 4px;
              color: #333;
              border: 1px solid #ccc;
            }
          }
        }
        .box-cover {
          height: 80px;
          display: flex;
          .box-left {
            width: 65px;
            line-height: 30px
            .cg {
              display: inline-block
            }
          }
          .box-right {
            display flex;
            justify-content: space-between;
            img, video {
              width: 80px;
              height: 80px;
              background: #eee;
            }
            .live-desc {
              height: 190px;
              border: 1px solid #cccccc !important;
            }
            select {
              height: 22px;
            }
            input[type=text] {
              width: 240px;
              height: 30px;
              border: 1px solid #cccccc;
              background: rgba(255, 255, 255, 1);
              opacity: 0.4;
              border-radius: 4px;
            }
            span {
              margin-left: 20px;
              margin-top: 52px;
              font-size: 11px;
              font-family: Adobe Heiti Std R;
              color: rgba(123, 123, 123, 1);
              line-height: 14px;
            }
            .input-live-password {
              color: red;
              margin-left: 10px;
            }
            .titleContent {
              width: 190px
            }
            .inputBox {
              width: 163px
            }
          }
        }
        .box-type {
          margin-top: 20px;
          height: 30px;
          display: flex;
          justify-content: flex-start;
          .box-left {
            width: 90px;
            line-height: 30px
            .cg {
              display: inline-block
            }
            .btn-upload {
              width: 65px;
              height: 25px;
              margin-top: 10px;
              color: rgba(0, 153, 249, 1);
              opacity: 0.4;
              border-radius: 2px;
              display: inline-block;
              text-align: center;
              cursor pointer;
              border: 1px solid #0099F9;
              background: #fff;
              line-height: 25px;
              padding: 0;

            }
          }
          .box-right {
            display flex;
            justify-content: space-between;
            img {
              width: 80px;
              height: 80px;
              background: #eee;
            }
            select {
              height: 22px;
              border: 1px solid #cccccc;
            }
            span {
              font-size: 11px;
              font-family: Adobe Heiti Std R;
              color: rgba(123, 123, 123, 1);
              line-height: 30px;
              margin-left: 10px;
            }
            .input-live-password {
              color: red;
              margin-left: 10px;
            }
            .titleContent {
              width: 190px
            }
            .inputBox {
              width: 163px
            }
            div {
              .name-upload {
                max-width: 100px;
                overflow: hidden;
                text-overflow: ellipsis;
                white-space: nowrap;
              }
            }
          }
        }
        .alert-content {
          height: 110px;
          display: flex;
          align-items: center;
          justify-content: center;
          font-family: "微软雅黑";
          font-size: 16px;

        }
      }

      .alert-bottom span {
        display: inline-block;
        text-align: center;
        height: 26px;
        width: 100px;
        line-height: 26px;
        cursor: pointer;
        border-radius: 4px;
      }
      .alert-bottom {
        margin: 25px;
        text-align: center;
      }
    }

  }
}


