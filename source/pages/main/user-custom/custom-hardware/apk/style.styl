  .apk-create-modal {
    .modal-content {
      width: 600px;
      .modal-header {
        .modal-title {
          font-size: 16px;
          color: #727272
        }
      }
      .modal-body {
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        .title {
          font-size: 16px;
          color: #727272
        }
        .form {
          .item {
            margin-bottom: 8px;
            .item-title {
              font-size: 12px;
              color: #000;
              margin-bottom: 5px;
            }
            input, select {
              width: 220px;
              height: 35px;
              border: 1px solid #ddd;
              padding-left: 10px;
              -webkit-border-radius: 5px;
              -moz-border-radius: 5px;
              border-radius: 5px;
              color: #7b7b7b;
              font-size: 12px;
            }
            .content-container {
              display: flex;
              .content-img-container {
                width: 74px;
                height: 90px;
                background: #ddd;
              }
              .select-btn {
                width: 70px;
                height: 30px;
                border: 1px solid #1d92f2;
                color: #1d92f2;
                -webkit-border-radius: 5px;
                -moz-border-radius: 5px;
                border-radius: 5px;
                line-height: 30px;
                text-align: center;
                cursor: pointer;
                margin-left: 16px;
                margin-top: 60px;
              }
              .select-btn:hover {
                background: #1d92f2
                color: #ffffff;
              }
            }
          }
          .item:first-child {
            margin-top: 20px;
          }
        }
        .error-tip {
          text-align: left;
          color: red;
          font-size: 12px;
        }
        .save-btn {
          color: #ffffff;
          background: #1d92f2;
          border: 1px solid #1d92f2;
          height: 35px;
          line-height: 35px;
          width: 220px;
          text-align: center;
          cursor: pointer;
          -webkit-border-radius: 4px;
          -moz-border-radius: 4px;
          border-radius: 4px;
          margin-top: 10px;
          margin-bottom: 30px;
        }
        .save-btn:hover {
          background: rgba(23, 149, 248, 1);
        }
      }
    }
  }
  .apk-version-modal {
    .modal-content {
      width: 700px;
      .modal-header {
        border-bottom: none;
        .modal-title {
          font-size: 16px;
          color: #727272
        }
      }
      .modal-body {
        .operation {
          display: flex;
          font-size: 12px;
          border-bottom: 1px solid #ddd;
          height: 40px;
          align-items: center;
          padding-bottom: 15px;
          .label {
            color: #000;
            margin-right: 5px;
          }
          .name {
            border: 1px solid #ddd;
            color: #8b8b8b;
            padding-left: 20px;
            padding-right: 20px;
            height: 25px;
            line-height:25px;
            text-align: center;
            margin-right: 10px;
            -webkit-border-radius: 3px;
            -moz-border-radius: 3px;
            border-radius: 3px;
          }
          .add-btn {
            color: #ffffff;
            background: #1d92f2;
            border: 1px solid #1d92f2;
            height: 25px;
            line-height: 25px;
            width: 80px;
            text-align: center;
            cursor: pointer;
            -webkit-border-radius: 4px;
            -moz-border-radius: 4px;
            border-radius: 4px;8
          }
          .add-btn:hover {
            background: rgba(23, 149, 248, 1);
          }
        }
        table {
          width: 100%;
          margin-top: 12px;
          margin-bottom: 20px;
          .table-header {
            color: #68788c;
            font-size: 14px;
            margin-bottom: 150px;
            td {
              min-width: 60px;
            }
          }
          .table-content {
            height: 40px;
            line-height: 40px;
            color: #68788c;
            font-size: 12px;
            td {
              min-width: 60px;
              margin-right: 20px;
            }
            .td-oper {
              display: flex;
              .update-btn {
                color: #1c8fec;
                margin-right: 20px;
                cursor: pointer;
              }
              .delete-btn {
                color: #f36f6d;
                margin-right: 30px;
                cursor: pointer;
              }
            }
          }
        }
        .page-info {

        }
      }
    }
  }
