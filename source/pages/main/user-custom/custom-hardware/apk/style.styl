.apk {
  .header {
    display: flex;
    height: 40px;
    align-items: center;
    .title {
      width: 15%;
      min-width: 150px;
      font-size: 16px;
    }
    .operation {
      width: 80%;
      display: flex;
      align-items: center;
      span {
        margin-right: 10px;
      }
      input {
        height: 30px;
        border-radius: 5px;
        border: 1px solid #ddd;
        padding-left: 5px;
        margin-right: 10px;
      }
      div {
        background: #1d94f5;
        height: 30px;
        line-height: 30px;
        color: #fff;
        border-radius: 3px;
        border: 1px solid #1d94f5;
        width: 70px;
        cursor: pointer;
        margin-right: 22px;
        text-align: center;
      }
    }
  }
  .hide-file {
    position: absolute;
    z-index: -100;
    display: none;
  }
  .body {
    table {
      width: 100%;
      margin-top: 10px;
      input[type="checkbox"] {
        width: 15px;
        height: 15px;
        cursor: pointer;
        margin-right: 10px;
      }
      thead {
        height: 40px;
        line-height: 40px;
        color: #68788c;
        border-bottom: 1px solid #ddd;
        .union-delete {
          color: #f36f6d;
          margin-left: 10px;
          cursor: pointer;
        }
        .union-delete:hover {
          color: red;
        }
        tr:first-child {
          width: 15%;
          th:first-child {
            display: flex;
            align-items: center;
            label {
              cursor: pointer;
              margin-bottom: 0;
            }
          }
        }
      }
      tbody {
        color: #9d9d9d;
        tr {
          height: 45px;
          line-height: 45px;
          .online-container {
            -webkit-border-radius: 5px;
            -moz-border-radius: 5px;
            border-radius: 5px;
            border: 1px solid #ddd;
            width: 90px;
            height: 30px;
            line-height: 26px;
            padding: 1px 1px;
            text-align: center;
          }
          td:first-child {
            width: 15%;
          }
          .up-line {
            background-color: #1d94f5;
            color: #ffffff;
            display: inline-block;
            height: 24px;
            line-height: 24px;
            pointer-events: none;
            padding-left: 5px;
            padding-right: 5px;
            -webkit-border-radius: 4px;
            -moz-border-radius: 4px;
            border-radius: 4px;
          }
          .down-line {
            background-color: #f36f6d;
            color: #ffffff;
            display: inline-block;
            height: 24px;
            line-height: 24px;
            pointer-events: none;
            padding-left: 5px;
            padding-right: 5px;
            -webkit-border-radius: 4px;
            -moz-border-radius: 4px;
            border-radius: 4px;
          }
          .is-click {
            cursor: pointer;
            color: #9d9d9d;
            height: 23px;
            background-color: #ffffff;
            pointer-events: auto;
          }
          .operation {
            color: #1d94f5;
            cursor: pointer;
            margin-right: 20px;
          }
          .delete {
            color: #f36f6d;
            margin-left: 20px;
            margin-right: 44px;
            cursor: pointer;
          }
        }
      }
    }
  }
  .footer {
    position: relative;
    margin-top: 10px;
    .page-text {
      position: absolute;
    }
    .page-info {
      height: 40px;
      line-height: 40px;
    }
    ul {
