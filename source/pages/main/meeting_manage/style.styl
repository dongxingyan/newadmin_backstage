main-meeting-manage {
  width: 100%
  height: 100%
  .ml20 {
    margin-left: 20px;
  }
  .main {
  //width: 100%
    position relative
    height: 100%
    display: flex
  //background: pink;
    flex-direction column
    font-family: "Helvetica Neue", "Luxi Sans", "DejaVu Sans", Tahoma, "Hiragino Sans GB", STHeiti, "Microsoft YaHei";
    font-size: 14px;
    .feedback{
      z-index 2
      position absolute
      width: 100%
      height: 100%
      display: flex
      justify-content center
      align-items center
      background:rgba(0,0,0,.5)
    }
    .alerts{
      width: 300px;
      height: 210px;
      background:#fff;
      border-radius: 5px;
      .alert-head{
        height: 40px;
        background: #31a3dd;
        line-height: 40px;
        padding-left: 35px;
        color: #ffffff;
        font-size: 12px;
        position: relative;
        border-radius: 5px 5px 0px 0px;
        .img-right,.delet-right,.add-right,.page-right{
          position: absolute;
          right: 6px;
          top: 8px;
          height: 25px;
        }
      }
      .img-left{
        position: absolute;
        left: 10px;
        top: 10px;
        height: 21px;
      }
      .alert-content{
        height: 110px;
        line-height: 110px;
        text-align: center;
        font-family: "微软雅黑";
        font-size: 16px;

      }
      .alert-bottom span{
        display: inline-block;
        text-align: center;
        padding: 5px;
        height: 30px;
        width: 80px;
        cursor: pointer;
        border-radius: 5px;
      }
      .alert-sure,.delet-sure,.add-sure,.page-sure{
        background:#31a3dd;
        color: #fff;
      }
      .alert-bottom{
        text-align: center;
        .alert-cancel{
          color:#fff;
          background:#969696;
        }
      }
    }

    .box{
      .title {
        font-size: 14px;
        height: 50px
        margin-left: 20px
        line-height: 50px
        background: url("../../../static/images2/react.png") left center no-repeat;
        display flex
        justify-content space-between
        span {
          padding-left: 10px;
        }
        .create {
          height: 30px
          line-height: 30px
          width: 80px
          border: none;
          background #0099CC
          margin-top: 10px;
          margin-right: 20px;
          color: #fff;
          font-size: 12px;
          border-radius: 3px;
          cursor: pointer;
        }
      }

      }
    }

}